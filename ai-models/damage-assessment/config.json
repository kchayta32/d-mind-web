
{
  "model_name": "damage-assessment-model",
  "version": "1.0.0",
  "description": "AI model for damage assessment analysis",
  "input_shape": [224, 224, 3],
  "output_classes": [
    "none",
    "minor", 
    "moderate",
    "severe",
    "critical"
  ],
  "preprocessing": {
    "resize": [224, 224],
    "normalize": true,
    "mean": [0.485, 0.456, 0.406],
    "std": [0.229, 0.224, 0.225]
  },
  "confidence_threshold": 0.5,
  "supported_formats": ["jpg", "jpeg", "png", "webp"]
}
